<!DOCTYPE html>
<html>
<head>
  <title>SocialMedia</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

<% if flash.present? %>
  <% flash.each do |key, value| %>
    <div class="<%= key %>">
      <%= value %>
    </div>
  <% end %>
<% end %>

<%= yield %>

</body>
</html>
